{
    NAME: "{{ dataset_name }}"        
    URL: "{{ dataset_url }}"
 
  // Geographic domain id (i.e., LINCOLN_PARK, 601.10)
  ATTRIBUTE_FIELD: {  
    COLUMN_ID: "{{ column_id }}}"
    TRANSFORM: {
      TYPE: REGEX
      ARGUMENTS: [
        "{{ regex_args }}"
      ]
    }
  }
    {% for indicator in indicators %}
    INDICATORS: [{
      DOMAIN: "{{ indicator.domain }}"
      NAME: "{{ indicator.name }}"
      UNITS: "{{ indicator.friendly_units }}"
      START_DATE: {{ indicator.start_date }}
      END_DATE: {{ indicator.end_date }}
      DESCRIPTION: "{{ indicator.description }}"
 
      // Statistical indicator (i.e., 3486)
        VALUE_FIELD: {
            COLUMN_ID: "{{ indicator.column }}"
            TRANSFORM: "{{ indicator.transform }}"
        }
    },
    ]
  {% endfor %}
}